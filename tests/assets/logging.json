{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s : %(levelname)s : %(message)s"
        }
    },
    "handlers": {
        "simpleFile": {
            "class": "logging.FileHandler",
            "formatter": "simple",
            "filename": "__tmp__/test.log"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": ["simpleFile"]
        }
    }
}